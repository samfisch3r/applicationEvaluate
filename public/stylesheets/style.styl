			border-radius 4px
			border-bottom 0
			color #ffffff
			text-align center
			text-decoration none
			z-index 3

			> :last-child
				margin 0

			&:hover
				color #ffffff !important

			h2
				margin 0

			.content
				transition max-height 0.5s ease, opacity 0.5s ease
				width 100%
				max-height 0
				line-height 1.5
				margin-top 0.35em
				opacity 0

				> :last-child
					margin-bottom 0

		&.style1 > .image:before
			background-color #f2849e

		&.style2 > .image:before
			background-color #7ecaf6

		&.style3 > .image:before
			background-color #7bd0c1

		&.style4 > .image:before
			background-color #c75b9b

		&.style5 > .image:before
			background-color #ae85ca

		&.style6 > .image:before
			background-color #8499e7

		body:not(.is-touch) .tiles article:hover > .image
			transform scale(1.1)

			&:before
				background-color #333333
				opacity 0.35

			&:after
				opacity 0

		body:not(.is-touch) .tiles article:hover .content
			max-height 15em
			opacity 1

	* + .tiles
